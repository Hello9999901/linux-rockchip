	bluetooth {
		compatible = "brcm,bcm4345c5";
		// clocks = <&hym8563>;
		// clock-names = "ext_clock";
		host-wakeup-gpios = <&gpio0 RK_PC5 GPIO_ACTIVE_HIGH>;
