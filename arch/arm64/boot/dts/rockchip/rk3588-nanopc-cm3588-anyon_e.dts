&edp1 {
    // analogix,video-bist-enable;
    // panel-self-test;
    pinctrl-names = "default";
    pinctrl-0 = <&edp1_hpd>;
    hpd-gpios = <&gpio3 RK_PB7 GPIO_ACTIVE_HIGH>;
    // force-hpd;
    status = "okay";

    ports {
    	port@1 {
          	reg = <1>;
            #address-cells = <1>;
            #size-cells = <0>;

            edp1_out_panel: endpoint@0 {
